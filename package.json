{
  "name": "ume-components",
  "version": "0.2.0",
  "description": "UME components",
  "homepage": "https://designteamproto.github.io/ume-components/",
  "license": "See license in LICENSE.md",
  "main": "dist/bundle.js",
  "dependencies": {
    "@material/animation": "^0.41.0",
    "@material/react-button": "^0.6.1",
    "@material/react-card": "^0.6.1",
    "@material/react-checkbox": "^0.6.1",
    "@material/react-ripple": "^0.6.1",
    "@material/theme": "^0.41.0",
    "classnames": "^2.2.6",
    "enzyme": "^3.7.0",
    "enzyme-adapter-react-16": "^1.6.0",
    "mini-css-extract-plugin": "^0.4.4",
    "npm-run-all": "^4.1.3",
    "prop-types": "^15.6.2",
    "react": "^16.6.0",
    "react-dom": "^16.6.0",
    "react-scripts": "2.1.0",
    "styled-components": "^3.4.9"
  },
  "scripts": {
    "build-css": "node-sass-chokidar --include-path ./src --include-path ./node_modules ./src/App.scss -o ./src",
    "watch-css": "yarn run build-css && node-sass-chokidar --include-path ./src --include-path ./node_modules --watch ./src/App.scss ./src/App.css",
    "start-js": "react-scripts start",
    "start": "npm-run-all -p watch-css start-js",
    "build-js": "react-scripts build",
    "build": "npm-run-all build-css build-js",
    "test": "react-scripts test --env=jsdom --verbose=true",
    "jest-test": "jest --ci --reporters=default --reporters=jest-junit",
    "eject": "react-scripts eject",
    "styleguide": "styleguidist server",
    "styleguide:build": "styleguidist build",
    "predeploy": "yarn run build",
    "deploy": "gh-pages -b gh-pages -d build",
    "transpile": "webpack"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "devDependencies": {
    "@babel/cli": "^7.1.2",
    "@babel/polyfill": "^7.0.0",
    "babel-loader": "^8.0.4",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-jsx-a11y": "^6.1.2",
    "eslint-plugin-react": "^7.11.1",
    "gh-pages": "^2.0.1",
    "jest": "^23.6.0",
    "jest-junit": "^5.2.0",
    "jest-mock": "^23.2.0",
    "node-sass-chokidar": "^1.3.4",
    "react-styleguidist": "^7.3.8",
    "react-test-renderer": "^16.5.2",
    "webpack-cli": "^3.1.2"
  }
}
